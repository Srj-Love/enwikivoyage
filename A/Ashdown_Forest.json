value":[["eat"],["name=The White Horse"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions="],["phone=+44 1342 850640"],["tollfree="],["fax="],["hours="],["price="],["content=Cowden,"]]}]},"\n",{"type":"===","value":["Tea rooms"]},"\n\nTea Rooms are an English tradition; expect high quality food and friendly staff:\n\n",{"type":"*","value":[" ",{"type":"{{","value":[["eat"],["name=Ashdown Forest Llama Park"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions=near Forest Row"],["phone=+44 1825 712040"],["tollfree="],["fax="],["hours="],["price="],["content="]]}]},"\n",{"type":"*","value":[" ",{"type":"{{","value":[["eat"],["name=Barnsgate Manor Vineyard"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions=near Uckfield"],["phone=+44 1825 713366"],["tollfree="],["fax="],["hours="],["price="],["content="]]}]},"\n",{"type":"*","value":[" ",{"type":"{{","value":[["eat"],["name=Duddleswell Tea Rooms"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions=near Uckfield"],["phone=+44 1825 712126"],["tollfree="],["fax="],["hours="],["price="],["content="]]}]},"\n",{"type":"*","value":[" ",{"type":"{{","value":[["eat"],["name=St Ives Tea Gardens"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions="],["phone=+44 1892 770589"],["tollfree="],["fax="],["hours="],["price="],["content=Hartfield,"]]}]},"\n",{"type":"*","value":[" ",{"type":"{{","value":[["eat"],["name=Wilderness Wood"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions=near Uckfield"],["phone=+44 1825 830509"],["tollfree="],["fax="],["hours="],["price="],["content="]]}]},"\n",{"type":"===","value":["Markets"]},"\n\nIf eating out isn't what you want but you still want to get a flavour of the area, visit local farm shops and farmers markets:\n\n",{"type":"*","value":[" ",{"type":"{{","value":[["eat"],["name=Deerview Products"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions="],["phone=+44 1825 721365"],["tollfree="],["fax="],["hours="],["price="],["content=Newick. Farm shop."]]}]},"\n",{"type":"*","value":[" ",{"type":"{{","value":[["eat"],["name=Nightingale Farm"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions="],["phone=+44 1342 825827"],["tollfree="],["fax="],["hours="],["price="],["content=Hartfield. Loy grazed meat."]]}]},"\nTowns such as ",{"type":"[[","value":[["Crowborough"]]},", ",{"type":"[[","value":[["Uckfield"]]},", ",{"type":"[[","value":[["East Grinstead"]]}," and Forest Row have both supermarkets and more specialist establishments.\n\n",{"type":"==","value":["Drink"]},"\n\n",{"type":"*","value":[" ",{"type":"{{","value":[["drink"],["name=Anchor Inn"],["url=http:\/\/www.anchorhartfield.com\/"],["email="],["address=Church Street, Hartfield TN7 4AG"],["lat="],["long="],["directions=In Hartfield village centre"],["phone=+44 1892 770424"],["tollfree="],["fax="],["hours=M-Sa 11AM-11PM Su 12PM-10.30PM"],["price="],["content="]]}]},"\n",{"type":"*","value":["  ",{"type":"{{","value":[["drink"],["name=The Blue Anchor"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions="],["phone=+44 1896 645519"],["tollfree="],["fax="],["hours="],["price="],["content=Crowborough,"]]}]},"\n",{"type":"*","value":["  ",{"type":"{{","value":[["drink"],["name=The Cat Inn"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions="],["phone=+44 1342 810369"],["tollfree="],["fax="],["hours="],["price="],["content=West Hoathly,"]]}]},"\n",{"type":"*","value":["  ",{"type":"{{","value":[["eat"],["name=Chequers Inn Hotel"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions="],["phone=+44 1342 823333"],["tollfree="],["fax="],["hours="],["price="],["content=Forest Row,"]]}]},"\n",{"type":"*","value":["  ",{"type":"{{","value":[["drink"],["name=The Coach and Horses"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions="],["phone=+