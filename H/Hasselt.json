"name=town park"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions="],["phone="],["tollfree="],["fax="],["hours="],["price="],["content="]]}]},"\n",{"type":"===","value":[" Museums "]},"\n",{"type":"*","value":[" ",{"type":"{{","value":[["see"],["name=National jenever museum"],["alt="],["url=http:\/\/www.jenevermuseum.be\/"],["email="],["address=Witte Nonnenstraat 19"],["lat=50.932726"],["long=5.340755"],["directions="],["phone="],["tollfree="],["fax="],["hours="],["price="],["content=Dutch\/Belgian version of gin"]]}]},{"type":"*","value":[" ",{"type":"{{","value":[["see"],["name=Urban Fashion museum"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions="],["phone="],["tollfree="],["fax="],["hours="],["price="],["content="]]}]},{"type":"*","value":[" ",{"type":"{{","value":[["see"],["name=Municipal Carillon Museum"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions="],["phone="],["tollfree="],["fax="],["hours="],["price="],["content="]]}]},{"type":"*","value":[" ",{"type":"{{","value":[["see"],["name=Museum of the 'Heilig Paterke'"],["alt="],["url="],["email="],["address="],["lat="],["long="],["directions="],["phone="],["tollfree="],["fax="],["hours="],["price="],["content="]]}]},"\n",{"type":"==","value":["Do"]},"\n",{"type":"===","value":[" Events "]},"\n",{"type":"*","value":[{"type":"{{","value":[["event"],["name=Jenever Festival"],["url= http:\/\/www.jeneverfeesten.be\/"],["year=2014"],["month=October"],["date= 18"],["endmonth="],["enddate= 19"],["location=Hasselt"],["country=Belgium"],["content="]]}]},"\n",{"type":"==","value":["Buy"]},"\n",{"type":"[[","value":[["File:Hasseltse jenever.jpg"],["thumbnail"],["Hasseltse Jenever"]]},"\nThe Demerstraat and the Koning Albertstraat are the most important shopping streets. In the Kapelstraat and the Hoogstraat are expensive shops with the most famous brands. The renovation of the TT-wijk twin towers complex (now including a shopping mall and a hotel) in 2003 gave the centre a new boost.\n\n",{"type":"==","value":["Eat"]},"\nThe \"Grote Markt\" (large central market square) and the nearby streets are lined with pubs, restaurants and taverns.\nTry mussels in the restaurants on the main streets or the chocolate ice cream store in the city center.\n\n",{"type":"==","value":["Drink"]},"\n",{"type":"==","value":["Sleep"]},"\n",{"type":"*","value":[" ",{"type":"{{","value":[["see"],["name=Express by Holiday Inn Hasselt"],["alt="],["url=http:\/\/www.ihg.com\/holidayinnexpress\/hotels\/us\/en\/hasselt\/hsltl\/hoteldetail"],["email="],["address=Thonissenlaan 37"],["lat=50.932925"],["long=5.335302"],["directions="],["phone="],["tollfree="],["fax="],["hours="],["price="],["content=Convenient and close to all amenities. Very limited parking and a walk to next public park house. Rooms standard European HI Express style. Breakfast basic but good."]]}]},"\n",{"type":"==","value":["Go next"]},"\n\n",{"type":"{{","value":[["outlinecity"]]},"\n\n",{"type":"{{","value":[["geo"],["50.9167"],["5.3333"]]},"\n\n",{"type":"{{","value":[["isPartOf"],["Limburg_(Belgium)"]]},"\n\n",{"type":"[[","value":[["WikiPedia:Hasselt"]]},"\n\n",{"type":"[[","value":[["Commons:Category:Hasselt"]]}]