ctions=SK22 2LE; follow Bank St then Kinder Rd for approx. 1 mile east from village (signposted)"],["phone =+44  1663 745394"],["tollfree="],["fax="],["hours="],["price="],["checkin="],["checkout="],["content=90 pitches; no caravans."]]}]},"\n",{"type":"==","value":["Connect"]},"\n",{"type":"*","value":[{"type":"{{","value":[["listing"],["name=Hayfield Countryside Centre"],["url="],["email="],["address=Station Road, Hayfield, High Peak SK22 2ES"],["lat=53.3792"],["long=-1.9473"],["directions="],["phone =+44  1663 746222"],["tollfree="],["fax="],["hours="],["price="],["content="]]}]},"\n",{"type":"==","value":["Go next"]},"\n",{"type":"*","value":[{"type":"[[","value":[["Buxton"]]}]},{"type":"*","value":[{"type":"[[","value":[["New Mills"]]}]},{"type":"*","value":[{"type":"[[","value":[["Glossop"]]}]},{"type":"*","value":[{"type":"[[","value":[["Edale"]]}]},{"type":"*","value":[{"type":"[[","value":[["Castleton"]]}]},"\n",{"type":"[[","value":[["WikiPedia:Hayfield"]]},"\n",{"type":"{{","value":[["Geo"],["53.3796"],["-1.9455"]]},"\n",{"type":"{{","value":[["IsPartOf"],["Derbyshire"]]},"\n",{"type":"{{","value":[["outlinecity"]]}]