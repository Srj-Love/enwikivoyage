":[["sleep"],["name=Lakeview Motel"],["url="],["email=lakeviewmotel@tnt21.com"],["address=378 Queen St"],["lat="],["long="],["directions="],["phone=+1 519-396-3301"],["tollfree="],["fax="],["hours="],["price="],["checkin="],["checkout="],["content="]]}]},"\n",{"type":"*","value":[" ",{"type":"{{","value":[["sleep"],["name=Living Waters Cabins"],["url=http:\/\/www.livingwaterscabins.ca"],["email="],["address=392 Queen St"],["lat="],["long="],["directions="],["phone=+1 519-396-3887"],["tollfree="],["fax="],["hours="],["price="],["checkin="],["checkout="],["content="]]}]},"\n",{"type":"*","value":[" ",{"type":"{{","value":[["sleep"],["name=Maple Leaf Inn"],["url="],["email="],["address=481 Broadway St"],["lat="],["long="],["directions="],["phone=+1 519-396-5888"],["tollfree="],["fax=+1 519-396-5868"],["hours="],["price="],["checkin="],["checkout="],["content="]]}]},"\n",{"type":"*","value":[" ",{"type":"{{","value":[["sleep"],["name=Save Inn Motel"],["url="],["email="],["address=1111 Sutton St"],["lat="],["long="],["directions="],["phone=+1 519-396-5454"],["tollfree="],["fax="],["hours="],["price="],["checkin="],["checkout="],["content="]]}]},"\n",{"type":"*","value":[" ",{"type":"{{","value":[["sleep"],["name=Southsider Motel"],["url="],["email="],["address=354 Queen St"],["lat="],["long="],["directions="],["phone=+1 519-396-3159"],["tollfree="],["fax="],["hours="],["price="],["checkin="],["checkout="],["content="]]}]},"\n",{"type":"===","value":[" Campgrounds "]},"\n\n",{"type":"*","value":[" ",{"type":"{{","value":[["sleep"],["name=Aintree Trailer Park"],["url=http:\/\/aintreepark.com"],["email="],["address=2435 Huron Concession 12 Rd"],["lat="],["long="],["directions="],["phone="],["tollfree=+1-877-396-8533"],["fax="],["hours="],["price="],["checkin="],["checkout="],["content="]]}]},"\n",{"type":"*","value":[" ",{"type":"{{","value":[["sleep"],["name=Bluewater Trailer Park"],["url="],["email="],["address=847 Saugeen Street"],["lat="],["long="],["directions="],["phone=+1 519-396-8698"],["tollfree="],["fax="],["hours="],["price="],["checkin="],["checkout="],["content=Great location right across from Dunsmoor Park and the beach."]]}]},"\n",{"type":"*","value":[" ",{"type":"{{","value":[["sleep"],["name=Green Acres Campground"],["url=http:\/\/www.greenacrescamp.com\/"],["email="],["address=Aintree Road"],["lat="],["long="],["directions="],["phone=+1 519-395-2808"],["tollfree="],["fax="],["hours="],["price="],["checkin="],["checkout="],["content="]]}]},"\n",{"type":"==","value":["Go next"]},"\n\nLarger nearby towns are ",{"type":"[[","value":[["Goderich"]]}," (~ 30 minutes by car south), ",{"type":"[[","value":[["Walkerton"]]}," (~ 30 minutes east), ",{"type":"[[","value":[["Port Elgin"]]}," (~ 30 minutes north).\n\nThe nearest major centres are ",{"type":"[[","value":[["London (Ontario)"],["London"]]}," (2 hours south-west), ",{"type":"[[","value":[["Kitchener"]]}," (1.5 hours east) or ",{"type":"[[","value":[["Owen Sound"]]}," (1.25 hours north)\n\n",{"type":"{{","value":[["geo"],["44.17489"],["-81.63616"]]},"\n",{"type":"{{","value":[["outlinecity"]]},"\n",{"type":"{{","value":[["isPartOf"],["Southwestern_Ontario"]]},"\n",{"type":"[[","value":[["wikipedia:Kincardine, Ontario"]]}]