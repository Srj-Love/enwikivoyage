[","value":[["Wadesboro"]]},". \n\n",{"type":"{{","value":[["routebox"],["image1=US 74.svg"],["imagesize1=22"],["directionl1=W"],["majorl1=",{"type":"[[","value":[["Charlotte"]]}],["minorl1=",{"type":"[[","value":[["Indian Trail"]]}],["directionr1=E"],["majorr1=",{"type":"[[","value":[["Lumberton"]]}],["minorr1=",{"type":"[[","value":[["Wadesboro"]]}]]},"\n\n",{"type":"{{","value":[["usablecity"]]},"\n\n",{"type":"{{","value":[["geo"],["34.988889"],["-80.549722"]]},"\n\n",{"type":"{{","value":[["isPartOf"],["Union County (North Carolina)"]]},"\n",{"type":"[[","value":[["Wikipedia:Monroe, North Carolina"]]},"\n\n",{"type":"[[","value":[["Dmoz:Regional\/North_America\/United_States\/North_Carolina\/Localities\/M\/Monroe\/"]]}]