ity of Luang Prabang.\n\n",{"type":"==","value":["See"]},"\n\n",{"type":"===","value":["Itineraries"]},"\n",{"type":"*","value":[" ",{"type":"[[","value":[["Ban Nalan Trail"]]}," - in Luang Namtha Province."]},"\n",{"type":"==","value":["Do"]},"\n\n",{"type":"==","value":["Eat"]},"\n\n",{"type":"==","value":["Drink"]},"\n\n",{"type":"==","value":["Stay safe"]}," \n\n",{"type":"==","value":["Go next"]},"\n",{"type":"*","value":[" ",{"type":"[[","value":[["Central Laos"]]}," to the south."]},{"type":"*","value":[" ",{"type":"[[","value":[["Thailand"]]}," has a popular border crossing in Bokeo province."]},{"type":"*","value":[" ",{"type":"[[","value":[["China"]]}," from Luang Prabang or Luang Namtha to ",{"type":"[[","value":[["Mengla"]]},"."]},"\n",{"type":"{{","value":[["outlineregion"]]},"\n",{"type":"{{","value":[["IsPartOf"],["Laos"]]},"\n",{"type":"{{","value":[["no Wikipedia link"]]}]