":"{{","value":[["outlineregion"]]},"\n\n",{"type":"{{","value":[["geo"],["25.002778"],["-107.502778"]]},"\n\n",{"type":"{{","value":[["IsPartOf"],["Northern Mexico"]]},"\n\n",{"type":"[[","value":[["WikiPedia:Sinaloa"]]},"\n",{"type":"[[","value":[["Dmoz:Regional\/North_America\/Mexico\/States\/Sinaloa\/"]]}]